                        <div class="panel-body"> 
<%= form_with(model: enquiry, local: true) do |form| %>
  <% if enquiry.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(enquiry.errors.count, "error") %> prohibited this enquiry from being saved:</h2>

      <ul>
      <% enquiry.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 <form class="form-horizontal">

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Full Name</label>
    <%= form.text_field :full_name, id: :enquiry_full_name,placeholder: 'Full Name', class: 'form-control' %>
  </div>
  </div>

 <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Email</label>
    <%= form.text_field :email, id: :enquiry_email,placeholder: 'Email', class: 'form-control' %>
  </div>
</div>


 <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Contact Number</label>
    <%= form.text_field :contact_no, id: :enquiry_contact_no,placeholder: 'Contact Number', class: 'form-control' %>
  </div>
</div>

 <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-10 control-label">Where did you find Us</label>
    <%= form.select :refarence_id, Refarence.all.collect{|a| [a.name, a.id]},{prompt:true},id: 'refarence_id',class: "form-control selectpicker", "data-live-search" => "true"  %>
  </div>
  </div>

<%if current_user.role == 'Center'%>
  <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-10 control-label">Counseling Done By</label>
    <%= form.select :employee_id, current_user.center.employees.collect{|a| [a.name, a.id]},{prompt:true},id: 'employee_id',class: "form-control selectpicker", "data-live-search" => "true"  %>
  </div>
  </div>
  <%else%>
  <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-10 control-label">Counseling Done By</label>
    <%= form.select :employee_id, Employee.all.collect{|a| [a.name, a.id]},{prompt:true},id: 'employee_id',class: "form-control selectpicker", "data-live-search" => "true"  %>
  </div>
  </div>
<%end%>

 <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Alternate Contact</label>
    <%= form.text_field :alternate_contact_no, id: :enquiry_alternate_contact_no,placeholder: 'Alternate Contact', class: 'form-control' %>
  </div>
  </div>




  
   <br><br><br><br><br><br>   <br><br>




                        <div class="form-group"> 
                                <div class="col-sm-offset-1 col-sm-10">  
     <%= form.submit "Submit", class: 'btn btn-success' %>
     <%= link_to 'Back', enquiries_path, class: 'btn btn-red' %>
     
  </div>
  </div>
 
<% end %>
</div>
</div></div></div>