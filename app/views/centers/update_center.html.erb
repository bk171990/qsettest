                                <li><a data-rel="close" href="#"><i class="icon-cancel"></i></a></li>
                            </ul> 
                        </div> 
                        <br>
                       
                        <div class="panel-body">  
 <%= form_for @center,method: :patch do |form| %>
  <% if @center.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@center.errors.count, "error") %> prohibited this @center from being saved:</h2>

      <ul>
      <% @center.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

 <form class="form-horizontal">

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Center Name</label>
    <%= form.text_field :center_name, id: :center_center_name, placeholder: 'Center Name', class: 'form-control' %>
    </div>
  </div>

      <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Center Address</label>
    <%= form.text_field :address, id: :center_address, placeholder: 'Center Address', class: 'form-control' %>
      </div>
    </div>

   <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">City</label>
          <%= form.text_field :city, id: :center_city , placeholder: 'City', class: 'form-control'%>
      </div>
  </div>

    <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Center State</label>
       
                  <%= form.select :state, options_for_select(["Andaman Nicobar Islands",
              "Andhra Pradesh","Arunachal Pradesh","Assam","Bihar","Chandigarh","Chhattisgarh","Dadra and Nagar Haveli", "Daman and Diu","Delhi","Goa", "Gujarat","Haryana","Himachal Pradesh", "Jammu and Kashmir", "Jharkhand", "Karnataka","Kerala","Lakshadweep", "Madhya Pradesh", "Maharashtra","Manipur","Meghalaya","Mizoram","Nagaland","Odisha","Puducherry","Punjab","Rajasthan","Sikkim","Tamil Nadu","Tripura","Uttar Pradesh","Uttarakhand", "West Bengal"
             ]),{prompt:true}, class: "form-control" %>
    </div>

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Pan Card No</label>
      <%= form.text_field :pan_card_no, id: :center_pan_card_no , placeholder: 'Pan Card No', class: 'form-control'%>
      </div>
    </div>

      <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Contact</label>
          <%= form.text_field :contact, id: :center_contact, placeholder: 'Contact', class: 'form-control' %>
      </div>
    </div>

      <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Alternate Contact</label>
        <%= form.text_field :alternate_contact, id: :center_alternate_contact, placeholder: 'Alternate Contact', class: 'form-control' %>
      </div>
      </div>

      <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-8 control-label">Center Starting Date</label>
         <%= form.date_field :center_starting_date, id: :center_center_starting_date, placeholder: 'Center Starting Date', class: 'form-control' %>
       </div>
      </div>


      <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-8 control-label">Contact Person Name</label>
    <%= form.text_field :contact_person_name, id: :center_contact_person_name, placeholder: 'Contact Person Name', class: 'form-control' %>
  </div>

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-8 control-label">Residential Address</label>
      <%= form.text_field :residential_address, id: :center_residential_address , placeholder: 'Residential Address', class: 'form-control'%>
     </div>
    </div>

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Email<font color="red">*</font></label>
        <%= form.text_field :email, id: :center_email, placeholder: 'Email', class: 'form-control' %>
        </div>
  </div>

   <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Center Code</label>
        <%= form.text_field :center_code, id: :center_center_code, placeholder: 'Center Code', readonly:true,class: 'form-control' %>
        </div>
  </div>

 
 <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Country</label>
          <%=country_select("center", "countries", { priority_countries: ["IN", "FR"], selected: "IN" }, { class: 'form-control', data: { attribute: "value" } })%>
        </div>
   </div>

  <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Status</label>
  <%= form.radio_button "center_status", "Active", checked: "checked" %> <b>Active</b>
              <%= form.radio_button "center_status", "Inactive"%> <b>Inactive</b>
  </div>
 </div>

  <br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
                        <div class="form-group"> 
                                <div class="col-sm-offset-1 col-sm-10">  
     <%= form.submit "Submit", class: 'btn btn-success' %>
    <%if current_user.role == 'Center'%>
<%= link_to 'Back', update_center_centers_path, class: 'btn btn-blue btn-outline'  %>
<%else%>
<%= link_to 'Back', centers_path, class: 'btn btn-blue btn-outline'  %>
<%end%>
     
  </div>
  </div>
 
<% end %>
</div>
</div></div></div>