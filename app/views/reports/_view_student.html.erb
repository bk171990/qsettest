<font color="green"> <b>Total registration Count For this Center is </b></font> <%=@center_students.count%> 
 <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover dataTables-example" >
            <thead>
              <tr>
              <th>Student Name</th>
              <th>University Name</th>
              <th>Course</th>
              <th>Enrollment No</th>
              <th></th>
             </tr>
           </thead>
           <tbody>
      
          <%unless @center_students.nil?%>
            <% @center_students.each do |b|%>
            <tr class="gradeX">
            <td><%= b.first_name+"-"+b.last_name%></td>
            <td><%= b.university.university_name%></td>
            <td><%= b.course.course_name %></td>
            <td><%= b.enrollment %></td>
            <td><%= link_to 'View Profile', student_path(b), class: 'btn btn-link'%></td>
            </tr>
           <%end%>
           <%end%>
          </tbody>
        </table>
      </div>
      <p>
          Download Report:
          <%#= link_to "CSV", student_s_home_url(format: "csv") %> |
          <%= link_to "Excel File", centers_url(format: "xls")  , class: "btn btn-success" %>
        </p>